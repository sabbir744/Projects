<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.sabbirtech.foodvillage.HomeActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:text="Burger Frenchfry"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView4"
        android:layout_centerHorizontal="true"
        android:text="Burger Vegetable"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView2"
        android:layout_alignTop="@+id/imageView3"
        android:text="Alamesa Cuba"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/textView3"
        android:layout_below="@+id/textView"
        android:text="250Tk"
        android:textAlignment="center"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/textView2"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="6dp"
        android:text="200 Tk"
        android:textAlignment="center"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/textView3"
        android:text="220 Tk"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="20dp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/textView"
        app:srcCompat="@drawable/cat1"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="18dp"
        app:srcCompat="@drawable/cat2" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="18dp"
        app:srcCompat="@drawable/cat3" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="14dp"
        app:srcCompat="@drawable/chicken_fry" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView5"
        android:layout_alignTop="@+id/imageView5"
        android:text="Chicken Fry"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/textView7"
        android:text="150 Tk"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:layout_alignParentStart="true"

        android:layout_marginTop="14dp"
         />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView8"
        android:layout_marginTop="6dp"
        app:srcCompat="@drawable/french_fry" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView8"
        android:layout_alignTop="@+id/imageView10"
        android:text="French Fry"
        android:textAlignment="center"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/textView9"
        android:layout_below="@+id/textView9"
        android:text="75 TK"
        android:textAlignment="center"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ImageButton
        android:id="@+id/image1_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignEnd="@+id/textView9"
        app:srcCompat="@drawable/buy" />

    <ImageButton
        android:id="@+id/image2_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignStart="@+id/image1_btn"
        android:background="@color/colorPrimary"
        app:srcCompat="@drawable/buy" />

    <ImageButton
        android:id="@+id/image3_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView5"
        android:layout_alignStart="@+id/image2_btn"
        android:background="@color/colorPrimary"
        app:srcCompat="@drawable/buy" />

    <ImageButton
        android:id="@+id/image4_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignEnd="@+id/textView9"
        android:background="@color/colorPrimary"
        app:srcCompat="@drawable/buy" />

    <ImageButton
        android:id="@+id/image5_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView10"
        android:layout_alignStart="@+id/image4_btn"
        android:background="@color/colorPrimary"
        app:srcCompat="@drawable/buy" />


</RelativeLayout>


